USER_VISIBLE_VERSION = 1.0.1
VERSION = 3.0

BASE_BUILD_FOLDER = $(SRCROOT)

SYMROOT = $(BASE_BUILD_FOLDER)/Build
OBJROOT = $(BASE_BUILD_FOLDER)/TemporaryItems

SDKROOT = iphoneos2.2.1
IPHONEOS_DEPLOYMENT_TARGET = 2.2.1

ARCHS = $(ARCHS_STANDARD_32_BIT)

INFINITELABS_DEVELOPMENT_IPHONE_CODE_SIGN_IDENTITY = iPhone Developer
INFINITELABS_DISTRIBUTION_IPHONE_CODE_SIGN_IDENTITY = iPhone Distribution

INFINITELABS_IPHONE_CODE_SIGN_IDENTITY = $(INFINITELABS_DEVELOPMENT_IPHONE_CODE_SIGN_IDENTITY)
CODE_SIGN_IDENTITY[sdk=iphoneos*] = $(INFINITELABS_IPHONE_CODE_SIGN_IDENTITY)

// Version info
INTERNAL_VERSION = $(VERSION)

// Silences IB.
IBC_NOTICES = NO
IBC_WARNINGS = NO

// Local settings. See the Scripts/ directory and the README for more info.
#include "Local.xcconfig"

